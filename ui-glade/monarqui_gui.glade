<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAction" id="action_configClose">
    <signal name="activate" handler="on_action_configClose_activate"/>
  </object>
  <object class="GtkAction" id="action_configOpen">
    <signal name="activate" handler="on_action_configOpen_activate"/>
  </object>
  <object class="GtkAction" id="action_mainExit">
    <property name="label">Exitj</property>
    <property name="stock_id">gtk-quit</property>
    <property name="icon_name">application-exit</property>
    <signal name="activate" handler="on_action_mainExit_activate"/>
  </object>
  <object class="GtkAction" id="action_startPause">
    <signal name="activate" handler="on_action_startPause_activate"/>
  </object>
  <object class="GtkImage" id="image_startStop">
    <property name="visible">True</property>
    <property name="icon_name">gtk-media-play-ltr</property>
  </object>
  <object class="GtkListStore" id="listStoreEntries">
    <columns>
      <!-- column-name path -->
      <column type="gchararray"/>
      <!-- column-name recursive -->
      <column type="gboolean"/>
      <!-- column-name event_create -->
      <column type="gboolean"/>
      <!-- column-name event_modify -->
      <column type="gboolean"/>
      <!-- column-name event_delete -->
      <column type="gboolean"/>
      <!-- column-name event_attrib -->
      <column type="gboolean"/>
      <!-- column-name event_moved_from -->
      <column type="gboolean"/>
      <!-- column-name event_moved_to -->
      <column type="gboolean"/>
      <!-- column-name ignore_files -->
      <column type="gchararray"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">sdfgsdfg</col>
        <col id="1">False</col>
        <col id="2">False</col>
        <col id="3">False</col>
        <col id="4">False</col>
        <col id="5">False</col>
        <col id="6">False</col>
        <col id="7">False</col>
        <col id="8" translatable="yes"></col>
      </row>
      <row>
        <col id="0" translatable="yes"></col>
        <col id="1">False</col>
        <col id="2">False</col>
        <col id="3">False</col>
        <col id="4">False</col>
        <col id="5">False</col>
        <col id="6">False</col>
        <col id="7">False</col>
        <col id="8" translatable="yes"></col>
      </row>
    </data>
  </object>
  <object class="GtkWindow" id="windowMain">
    <property name="title" translatable="yes">Monarqui 0.1.3 - Main</property>
    <property name="window_position">center</property>
    <signal name="destroy" handler="on_windowMain_destroy"/>
    <child>
      <object class="GtkVBox" id="vbox1">
        <property name="visible">True</property>
        <child>
          <object class="GtkMenuBar" id="menubar">
            <property name="visible">True</property>
            <child>
              <object class="GtkMenuItem" id="menuitem1">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_File</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="menuArquivo">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkImageMenuItem" id="menuItemExit">
                        <property name="visible">True</property>
                        <property name="use_action_appearance">True</property>
                        <property name="related_action">action_mainExit</property>
                        <property name="use_stock">True</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkToolbar" id="toolbar">
            <property name="visible">True</property>
            <property name="icon_size">8</property>
            <child>
              <object class="GtkToolButton" id="btnEntryAdd">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">action_entryAdd</property>
                <property name="label" translatable="yes">startStop</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="btnEntryModify">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">action_entryModify</property>
                <property name="label" translatable="yes">Add</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="btnEntryDelete">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">action_entryDelete</property>
                <property name="label" translatable="yes">Modify</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="btnStart">
                <property name="visible">True</property>
                <property name="related_action">action_startPause</property>
                <property name="label" translatable="yes">startStop</property>
                <property name="use_underline">True</property>
                <property name="icon_widget">image_startStop</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
            <child>
              <object class="GtkToolButton" id="btnSaveConfig">
                <property name="visible">True</property>
                <property name="use_action_appearance">True</property>
                <property name="related_action">action_saveConfig</property>
                <property name="label" translatable="yes">startStop</property>
                <property name="use_underline">True</property>
              </object>
              <packing>
                <property name="expand">False</property>
                <property name="homogeneous">True</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkNotebook" id="notebook1">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <child>
              <object class="GtkTreeView" id="treeview2">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="model">listStoreEntries</property>
                <property name="headers_clickable">False</property>
                <property name="search_column">0</property>
                <property name="enable_grid_lines">both</property>
                <property name="enable_tree_lines">True</property>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn1">
                    <property name="resizable">True</property>
                    <property name="title">Path</property>
                    <child>
                      <object class="GtkCellRendererText" id="cellrenderertext1"/>
                      <attributes>
                        <attribute name="editable">1</attribute>
                        <attribute name="text">0</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn2">
                    <property name="resizable">True</property>
                    <property name="title">Recursive</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle1"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                        <attribute name="activatable">1</attribute>
                        <attribute name="active">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="column3">
                    <property name="title">Create</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle2"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn3">
                    <property name="title">Modify</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle3"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn4">
                    <property name="title">Delete</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle4"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn5">
                    <property name="title">Attrs</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle5"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn6">
                    <property name="title">Moved From</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle6"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn7">
                    <property name="title">Moved To</property>
                    <child>
                      <object class="GtkCellRendererToggle" id="cellrenderertoggle7"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkTreeViewColumn" id="treeviewcolumn8">
                    <property name="title">Ignores</property>
                    <child>
                      <object class="GtkCellRendererText" id="cellrenderertext2"/>
                      <attributes>
                        <attribute name="visible">1</attribute>
                      </attributes>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child type="tab">
              <object class="GtkLabel" id="labelTabEntries">
                <property name="visible">True</property>
                <property name="label" translatable="yes">Entries</property>
              </object>
              <packing>
                <property name="tab_fill">False</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkAction" id="action_entryAdd">
    <property name="label">Add</property>
    <property name="stock_id">list-add</property>
    <property name="icon_name">list-add</property>
    <property name="always_show_image">True</property>
    <signal name="activate" handler="on_action_entryAdd_activate"/>
  </object>
  <object class="GtkAction" id="action_entryModify">
    <property name="label">Modify</property>
    <property name="stock_id">insert-text</property>
    <property name="icon_name">applications-system</property>
    <property name="always_show_image">True</property>
    <signal name="activate" handler="on_action_entryModify_activate"/>
  </object>
  <object class="GtkAction" id="action_entryDelete">
    <property name="label">Delete</property>
    <property name="stock_id">list-remove</property>
    <property name="icon_name">list-remove</property>
    <property name="always_show_image">True</property>
    <signal name="activate" handler="on_action_entryDelete_activate"/>
  </object>
  <object class="GtkAction" id="action_saveConfig">
    <property name="stock_id">gtk-save</property>
    <property name="icon_name">document-save</property>
    <property name="always_show_image">True</property>
    <signal name="activate" handler="on_action_saveConfig_activate"/>
  </object>
  <object class="GtkWindow" id="windowEntry">
    <child>
      <object class="GtkVBox" id="vbox3">
        <property name="visible">True</property>
        <child>
          <placeholder/>
        </child>
        <child>
          <placeholder/>
        </child>
      </object>
    </child>
  </object>
  <object class="GtkImage" id="image_add">
    <property name="visible">True</property>
    <property name="icon_name">list-add</property>
  </object>
  <object class="GtkImage" id="image_modify">
    <property name="visible">True</property>
    <property name="icon_name">insert-text</property>
  </object>
  <object class="GtkImage" id="image_delete">
    <property name="visible">True</property>
    <property name="icon_name">list-remove</property>
  </object>
  <object class="GtkImage" id="image_startStop1">
    <property name="visible">True</property>
    <property name="icon_name">gtk-media-play-ltr</property>
  </object>
</interface>
